<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane minHeight="630.0" minWidth="600.0" prefHeight="630.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fx.ControllerSample">
   <children>
      <HBox fillHeight="false" minHeight="300.0" minWidth="600.0" prefHeight="300.0" prefWidth="600.0" snapToPixel="false" stylesheets="style.css">
          <children>
              <VBox minHeight="300.0" minWidth="350.0" prefHeight="300.0" prefWidth="350.0">
                  <children>
                      <Label minHeight="20.0" text="Root">
                          <VBox.margin>
                              <Insets bottom="3.0" left="3.0" right="5.0" top="5.0" />
                          </VBox.margin>
                          <font>
                              <Font size="14.0" />
                          </font>
                      </Label>
                      <TreeView fx:id="tree" minHeight="269.0" minWidth="300.0" onContextMenuRequested="#ContextMenuEvent" prefHeight="269.0" prefWidth="300.0">
                          <opaqueInsets>
                              <Insets />
                          </opaqueInsets>
                          <VBox.margin>
                              <Insets left="3.0" right="3.0" />
                          </VBox.margin>
                      </TreeView>
                  </children>
              </VBox>
              <VBox minHeight="300.0" minWidth="250.0" prefHeight="300.0" prefWidth="250.0">
      
                        <children>
                      <VBox minHeight="250.0" minWidth="250.0" prefHeight="250.0" prefWidth="250.0">
                          <children>
                              <Label alignment="TOP_CENTER" contentDisplay="CENTER" text="Direction">
                                  <VBox.margin>
                                      <Insets bottom="3.0" left="3.0" top="5.0" />
                                  </VBox.margin>
                        </Label>
                              <TextField fx:id="textfieldDir" accessibleHelp="C://dir/        or //SERVER/dir" minHeight="25.0" minWidth="250.0" prefWidth="250.0" promptText="Root/branch/directory for search" text="files">
                                  <opaqueInsets>
                                      <Insets />
                                  </opaqueInsets>
                                  <VBox.margin>
                                      <Insets right="3.0" />
                                  </VBox.margin>
                              </TextField>
                              <Label alignment="TOP_CENTER" contentDisplay="CENTER" text="Text">
                                  <VBox.margin>
                                      <Insets bottom="3.0" left="3.0" top="5.0" />
                                  </VBox.margin>
                              </Label>
                              <TextArea fx:id="textfieldText" minHeight="50.0" minWidth="250.0" prefHeight="90.0" prefWidth="250.0" promptText="Text for search, LINE FEED is not considered." text="Это город Москва." VBox.vgrow="ALWAYS">
                                  <VBox.margin>
                                      <Insets right="3.0" />
                                  </VBox.margin>
                              </TextArea>
                              <HBox minWidth="210.0" prefHeight="45.0" prefWidth="210.0">
                                  <children>
                                      <Label minWidth="85.0" prefHeight="17.0" prefWidth="85.0" text="File extension">
                                          <HBox.margin>
                                              <Insets bottom="10.0" left="3.0" top="13.0" />
                                          </HBox.margin>
                                      </Label>
                                      <TextField fx:id="textfieldExt" accessibleRole="TEXT" alignment="CENTER" minHeight="25.0" minWidth="60.0" prefHeight="25.0" prefWidth="60.0" promptText="log" text="txt">
                                          <HBox.margin>
                                              <Insets bottom="10.0" left="18.0" top="10.0" />
                                          </HBox.margin>
                                      </TextField>
                                      <Button fx:id="button" alignment="CENTER" contentDisplay="TOP" minWidth="65.0" mnemonicParsing="false" onMouseClicked="#buildTree" prefWidth="65.0" text="Search">
                                          <HBox.margin>
                                              <Insets left="18.0" right="3.0" top="10.0" />
                                          </HBox.margin>
                                      </Button>
                                  </children>
                              </HBox>
                          </children>
                      </VBox>
                      <HBox disable="true" minHeight="50.0" prefHeight="50.0" prefWidth="250.0">
                          <children>
                              <Label fx:id="warningLabel" text="Label" />
                          </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </HBox>
                  </children>
              </VBox>
          </children>
      </HBox>
      <VBox alignment="TOP_CENTER" layoutY="300.0" minHeight="330.0" minWidth="600.0" prefHeight="330.0" prefWidth="600.0">
         <children>
            <TabPane fx:id="tabPane" minWidth="600.0" prefHeight="330.0" prefWidth="600.0" side="BOTTOM">
              <tabs>
                <Tab text="Untitled Tab 1">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </children>
      </VBox>
   </children>
</Pane>
